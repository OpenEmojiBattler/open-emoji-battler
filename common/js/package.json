{
  "private": true,
  "name": "common",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./src/index.ts",
  "//": "Before executing the generate:defs, uncomment the assertion in src/interfaces/definitions.ts",
  "scripts": {
    "generate:defs": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' ts-node --project './tsconfig.base.json' --esm node_modules/.bin/polkadot-types-from-defs --package '.' --input './src/interfaces'",
    "lint": "prettier --write '{src,scripts}/**/*'",
    "check-ts": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@polkadot/api-contract": "10.6.1",
    "@polkadot/typegen": "10.6.1",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
