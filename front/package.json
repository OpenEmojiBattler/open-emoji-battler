{
  "private": true,
  "name": "front",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "OEB_ENV=local webpack serve --mode development --hot",
    "build": "rm -rf ./dist && ./scripts/put-redirects.sh && OEB_ENV=production webpack --mode production",
    "lint": "prettier --write '{src,scripts}/**/*'",
    "update-wasm": "wasm-pack build --target bundler --release --out-dir pkgbundler wasm && wasm-pack build --target nodejs --release --out-dir pkgnodejs wasm && ts-node scripts/generateSrcWasm.ts",
    "check-ts": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@babel/core": "7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@polkadot/extension-dapp": "0.37.3-19",
    "@polkadot/ui-shared": "0.77.1",
    "@types/react-dom": "17.0.0",
    "babel-loader": "8.2.2",
    "buffer": "6.0.3",
    "common": "0.1.0",
    "crypto-browserify": "3.12.0",
    "css-loader": "5.0.1",
    "html-webpack-plugin": "5.0.0-beta.6",
    "mini-css-extract-plugin": "1.3.4",
    "process": "0.11.10",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-refresh": "0.9.0",
    "sass": "1.32.0",
    "sass-loader": "10.1.1",
    "stream-browserify": "3.0.0",
    "ts-loader": "8.0.14",
    "wasm-pack": "0.9.1",
    "webpack": "5.17.0",
    "webpack-cli": "4.4.0",
    "webpack-dev-server": "3.11.2",
    "worker-loader": "3.0.7"
  }
}
